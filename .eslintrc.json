{
  "extends": "./node_modules/gts/",
  "parserOptions": {
    "warnOnUnsupportedTypeScriptVersion": false
  },
  "rules": {
    "eqeqeq": ["error", "always", { "null": "ignore" }],
    "@typescript-eslint/no-unused-vars": [
      "error",
      { "ignoreRestSiblings": true }
    ],
    "prettier/prettier": "warn"
  }
}
