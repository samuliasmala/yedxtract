{
  "name": "yedxtract",
  "version": "0.0.0",
  "description": "Extract texts from yEd graph editor to Excel and back",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "test": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "compile:watch": "tsc --watch --preserveWatchOutput",
    "prestart": "npm run compile",
    "start": "node build/index.js",
    "dev": "concurrently --kill-others --names app,tsc \"nodemon build/index.js\" \"npm run compile:watch\"",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^14.11.2",
    "concurrently": "^7.2.0",
    "gts": "^3.1.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.0.3"
  }
}
